{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-toaster@5.1.0",
      "destination": "wwwroot/lib/bootstrap-toaster/",
      "files": [
        "css/bootstrap-toaster.css",
        "js/bootstrap-toaster.js",
        "js/bootstrap-toaster.min.js",
        "css/bootstrap-toaster.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}