                                <li><a href="#" class="twitter"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="#" class="linkedin"><i class="fa fa-linkedin"></i></a></li>
                                <li><a href="#" class="gp"><i class="fa fa-google-plus"></i></a></li>
                                <li><a href="#" class="skype"><i class="fa fa-skype"></i></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="single-footer-widget">
                            <h3>recent post</h3>
                            <div class="latest-post-footer clearfix">
                                <div class="latest-post-footer-left">
                                    <img src="/Front/assets/img/footer-post-2.jpg" alt="post" />
                                </div>
                                <div class="latest-post-footer-right">
                                    <h4><a href="#">Website design trends for 2018</a></h4>
                                    <p>January 14 - 2018</p>
                                </div>
                            </div>
                            <div class="latest-post-footer clearfix">
                                <div class="latest-post-footer-left">
                                    <img src="/Front/assets/img/footer-post-3.jpg" alt="post" />
                                </div>
                                <div class="latest-post-footer-right">
                                    <h4><a href="#">UI experts and modern designs</a></h4>
                                    <p>January 12 - 2018</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="single-footer-widget">
                            <h3>main links</h3>
                            <ul>
                                <li><a href="#"><i class="fa fa-angle-double-right "></i> About jobguru</a></li>
                                <li><a href="#"><i class="fa fa-angle-double-right "></i> Delivery Information</a></li>
                                <li><a href="#"><i class="fa fa-angle-double-right "></i> Terms & Conditions</a></li>
                                <li><a href="#"><i class="fa fa-angle-double-right "></i> Customer support</a></li>
                                <li><a href="#"><i class="fa fa-angle-double-right "></i> Contact with an expert</a></li>
                                <li><a href="#"><i class="fa fa-angle-double-right "></i> community updates</a></li>
                                <li><a href="#"><i class="fa fa-angle-double-right "></i> upcoming updates</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="single-footer-widget footer-contact">
                            <h3>Contact Info</h3>
                            <p><i class="fa fa-map-marker"></i> 4257 Street, SunnyVale, USA </p>
                            <p><i class="fa fa-phone"></i> 012-3456-789</p>
                            <p><i class="fa fa-envelope-o"></i> info@jobguru.com</p>
                            <p><i class="fa fa-envelope-o"></i> info@jobguru.com</p>
                            <p><i class="fa fa-fax"></i> 112-3456-7898</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="footer-copyright">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="copyright-left">
                            <p>Copyright &copy; 2018 Themescare. All Rights Reserved</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- Footer Area End -->
    <!--Jquery js-->
    <script src="/Front/assets/js/jquery-3.6.0.min.js"></script>
    <!--Bootstrap js-->
    <script src="/Front/assets/js/bootstrap.bundle.min.js"></script>
    <!--Bootstrap Datepicker js-->
    <script src="/Front/assets/js/bootstrap-datepicker.min.js"></script>
    <!--Perfect Scrollbar js-->
    <script src="/Front/assets/js/jquery-perfect-scrollbar.min.js"></script>
    <!--Owl-Carousel js-->
    <script src="/Front/assets/js/owl.carousel.min.js"></script>
    <!--SlickNav js-->
    <script src="/Front/assets/js/jquery.slicknav.min.js"></script>
    <!--Magnific js-->
    <script src="/Front/assets/js/jquery.magnific-popup.min.js"></script>
    <!--Select2 js-->
    <script src="/Front/assets/js/select2.min.js"></script>
    <!--jquery-ui js-->
    <script src="/Front/assets/js/jquery-ui.min.js"></script>
    <!--Jarallax js-->
    <script src="/Front/assets/js/jarallax.min.js"></script>
    <script src="/Front/assets/js/jarallax-video.min.js"></script>
    <!--Main js-->
    <script src="/Front/assets/js/main.js"></script>
    <!--Toaster js-->
    <script src="/lib/Toaster/js/toasting.js"></script>
    <!--RichText js-->
    <script src="/lib/RichText/jquery.richtext.js"></script>
    <!--Site js-->
    <script src="/js/site.js" asp-append-version="true"></script>

    @if (viewResult != null)
    {
        <script>
            $(document).ready(function () {
                let type = @(viewResult.Success ? 1 :0);
                let message = '@viewResult.Message';
                let delay = @viewResult.Delay;
                ShowToaster(type, message);
            });
        </script>
    }
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>