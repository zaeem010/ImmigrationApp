@{
    var viewResult = TempData["ToasterResult"] != null ? JsonSerializer.Deserialize<ToasterResult>(TempData["ToasterResult"].ToString()) : null;
}
<!DOCTYPE html>
<html lang="en-US">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Job Portal">
    <meta name="keyword" content="Job, freelancer, employee, marketplace">
    <meta name="author" content="N">
    <!-- Title -->
    <title>@ViewData["Title"] | Job Portal </title>
    <!-- Favicon -->
    <link rel="icon" type="image/png" sizes="32x32" href="/Front/assets/img/favicon/favicon-32x32.png">
    <!--Bootstrap css-->
    <link rel="stylesheet" href="/Front/assets/css/bootstrap.css">
    <!--Font Awesome css-->
    <link rel="stylesheet" href="/Front/assets/css/font-awesome.min.css">
    <!--Magnific css-->
    <link rel="stylesheet" href="/Front/assets/css/magnific-popup.css">
    <!--Owl-Carousel css-->
    <link rel="stylesheet" href="/Front/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="/Front/assets/css/owl.theme.default.min.css">
    <!--Animate css-->
    <link rel="stylesheet" href="/Front/assets/css/animate.min.css">
    <!--Select2 css-->
    <link rel="stylesheet" href="/Front/assets/css/select2.min.css">
    <!--Slicknav css-->
    <link rel="stylesheet" href="/Front/assets/css/slicknav.min.css">
    <!--Bootstrap-Datepicker css-->
    <link rel="stylesheet" href="/Front/assets/css/bootstrap-datepicker.min.css">
    <!--Jquery UI css-->
    <link rel="stylesheet" href="/Front/assets/css/jquery-ui.min.css">
    <!--Perfect-Scrollbar css-->
    <link rel="stylesheet" href="/Front/assets/css/perfect-scrollbar.min.css">
    <!--Site Main Style css-->
    <link rel="stylesheet" href="/Front/assets/css/style.css">
    <!--Responsive css-->
    <link rel="stylesheet" href="/Front/assets/css/responsive.css">
    <!--Toater css-->
    <!--Toater css-->
    <link href="/lib/Toaster/css/toasting.css" rel="stylesheet">
    <!--Rich Text css-->
    <link href="/lib/RichText/richtext.min.css" rel="stylesheet">
    <!--Jquery Add-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js" asp-append-version="true"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</head>
<body>

    @RenderBody()
